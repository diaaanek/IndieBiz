<h1>login </h1><br>

<div class="wrapper">
  <%= form_tag sessions_path do %>
    <%= text_field_tag :email, nil, placeholder: "Email" %><br>
    <%= password_field_tag :password, nil,  placeholder: "Password" %><br>

    <%= submit_tag "Login" %>
  <% end %>
  <br><br>

  <p> Don't have an account? <%= link_to "Sign Up", new_user_path%> 
</div>
