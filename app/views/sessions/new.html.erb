<br><br>
<p id=”alert”><%= alert %></p>

<h1>Login</h1><br>

<div class="wrapper">
  <%= form_tag sessions_path do |form| %>
      <div class=”field”>
        <%= label_tag :email %>
        <%= text_field_tag :email  %><br>
      </div>

      <div class=”field”>
      <%= label_tag :password %>
      <%= password_field_tag :password  %><br>
      </div>

     <div class=”actions”>
        <%= submit_tag "Login" %>
      </div>

      <% end %>

  <p> Don't have an account? <%= link_to "Sign Up", new_user_path %>
</div>
